<script>

// Support for jQuery features
// Steve Knoblock Saturday, June 06, 2009

$(document).ready(function() {

	// profile collapse and expand
	$('a.toggle1').click(function(){
		$('#more_info1').toggle();
			moretext = this.text;
			if ( moretext == 'Read more' ) { moretext = 'Close'; } else { moretext = 'Read more'; }
			$('#more_link').text(moretext);
			return false;
		});


	// profile collapse and expand
	$('a.toggle2').click(function(){
		$('#more_info2').toggle();
			moretext = this.text;
			if ( moretext == 'Open' ) { moretext = 'Close'; } else { moretext = 'Open'; }
			$('#more_link2').text(moretext);
			return false;
		});
		
		
		
	// profile collapse and expand
	$('a.toggle3').click(function(){
		$('#more_info3').toggle();
			moretext = this.text;
			if ( moretext == 'Open' ) { moretext = 'Close'; } else { moretext = 'Open'; }
			$('#more_link3').text(moretext);
			return false;
		});
});

$( document ).ready(function() {
    console.log( "ready!" );
});
</script>
